{
  "name": "Aysha",
  "description": "Advanced Database Backend Chatbot Based On Pyrogram Userbot",
  "logo": "https://telegra.ph/file/c8510dc787aa503705ea1.jpg",
  "keywords": [
    "telegram",
    "bot",
    "python",
    "pyrogram"
  ],
    "env": {
        "SESSION_NAME": {
            "description": "Pyrogram string session get it from @Venom_stringbot",
            "required": true
        },        
        "MONGO_URL": {
            "description": "mongo db url here",
            "required": true
        },
        "API_ID": {
            "description": "telegram api id here",
            "required": true
        },
        "API_HASH": {
            "description": "telegram api hash here",
            "required": true
        }
    },
    "addons": [
        {
            "plan": "heroku-postgresql"
        }
    ]
}
